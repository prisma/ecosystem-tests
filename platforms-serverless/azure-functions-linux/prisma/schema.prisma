generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
  previewFeatures = ["napi"]
}

datasource db {
  provider = "postgresql"
  url      = env("AZURE_FUNCTIONS_LINUX_PG_URL")
}

model User {
  id    String  @default(cuid()) @id
  email String  @unique
  name  String?
}
