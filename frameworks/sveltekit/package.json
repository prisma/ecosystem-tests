{
  "name": "kit-prisma",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build && cp node_modules/prisma/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "vercel-build": "prisma generate && prisma migrate deploy && yarn build"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.73",
    "@sveltejs/adapter-vercel": "^1.0.0-next.47",
    "@sveltejs/kit": "next",
    "prisma": "3.14.0-integration-tnzk-feature-esm-friendly-reexports.2",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "3.14.0-integration-tnzk-feature-esm-friendly-reexports.2"
  }
}
