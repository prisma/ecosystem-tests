{
  "name": "nexus",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server"
  },
  "dependencies": {
    "@nexus/schema": "0.14.0",
    "graphql": "14.6.0",
    "graphql-yoga": "1.18.3",
    "nexus-plugin-prisma": "0.16.1"
  },
  "devDependencies": {
    "@types/node": "12.12.50",
    "@types/react": "16.9.43",
    "@types/ws": "7.2.6",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.52",
    "typescript": "3.9.7"
  },
  "resolutions": {
    "@prisma/client": "2.3.0-dev.49",
    "@prisma/cli": "2.3.0-dev.49"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
