generator client {
  provider        = "prisma-client-js"
  binaryTargets   = ["native", "rhel-openssl-1.0.x"]
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  // fake url to ensure the driver adapter is used
  url      = env("INVALID_ENV_VAR")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
  age   Int?
}
